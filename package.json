{
  "name": "keep-active-group-active",
  "displayName": "keep-active-group-active",
  "description": "Commands to open existing text editors in the active editor group",
  "publisher": "jonathanjameswatson",
  "version": "0.0.1",
  "homepage": "https://github.com/jonathanjameswatson/keep-active-group-active",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonathanjameswatson/keep-active-group-active"
  },
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "keep-active-group-active.revert-next-active-text-editor-view-column",
        "title": "Revert Next Active Text Editor View Column"
      },
      {
        "command": "keep-active-group-active.cancel",
        "title": "Do Not Revert Next Active Text Editor View Column"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@types/vscode": "^1.97.0",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3",
    "ts-loader": "^9.5.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  }
}
